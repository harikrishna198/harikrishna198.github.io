    <h3>output:</h3><p id="txt15"></p><hr>
    <h3>output:</h3><p id="txt16"></p><hr>
<script type="text/javascript">
document.getElementById("txt1").innerHTML="Wishdom".toUpperCase();
document.getElementById("txt2").innerHTML="Devine".length;
document.getElementById("txt3").innerHTML="Helps someones it will defenitely reflects to u".search("good");
document.getElementById("txt4").innerHTML="live is like bicycle".indexOf("passion");
